{
    "name": "hudhaifas/silverstripe-dataobject-manager",
    "description": "Wrap any Silverstripe DataObject in a page.",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/hudhaifas/silverstripe-dataobject-manager",
    "keywords": ["silverstripe", "dataobject", "page"],
    "license": "MIT",
    "authors": [
        {
            "name": "Hudhaifa Shatnawi",
            "email": "me@hudhaifa.com"
        }
    ],
    "support": {
        "issues": "https://github.com/hudhaifas/silverstripe-dataobject-manager/issues"
    },
    "require": {
        "silverstripe/framework": "^5",
        "silverstripe/cms": "^5",
        "silverstripe/vendor-plugin": "~2.0.1@stable",
        "hudhaifas/silverstripe-frontend-fields": "5.*",
        "hudhaifas/silverstripe-data-progress": "5.*"
    },
    "extra": {
        "installer-name": "dataobject-manager",
        "expose": [
            "res/css",
            "res/images",
            "res/js"
        ]
    }
}
